<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="_Menu" prefHeight="400.0" prefWidth="600.0" stylesheets="@Main.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.MainMenuController">
   <children>
      <VBox alignment="CENTER_RIGHT" layoutX="241.0" layoutY="100.0" prefHeight="200.0" prefWidth="100.0" spacing="40.0">
         <children>
            <ButtonBar prefHeight="40.0" prefWidth="200.0">
              <buttons>
                <Button mnemonicParsing="false" onAction="#switchToPlay" text="Play" />
              </buttons>
            </ButtonBar>
            <Button mnemonicParsing="false" onAction="#switchToAudio" stylesheets="@Main.css" text="Directory" />
            <Button mnemonicParsing="false" onAction="#switchToCreate" text="Create" />
         </children>
      </VBox>
   </children>
</AnchorPane>
